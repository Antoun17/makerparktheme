{{attach_library('makerparktheme/bootstrap') }}
{{attach_library('makerparktheme/popper')}}
{{attach_library('makerparktheme/videojs')}}

<link href="node_modules/video.js/dist/video-js.min.css" rel="stylesheet">
<script src="node_modules/videojs-vtt.js/dist/videojs-vtt.js"></script>
<script src="node_modules/video.js/dist/video.min.js"></script>
<script>
  videojs.options.flash.swf = "node_modules/video.js/dist/video-js.swf"
</script>


<div class="main-navigation">
{{page.main_navigation}}
</div>


<div class="header">
  {{ page.header }}
</div>


  <div class="livestream">
  <div class="embed-responsive embed-responsive-16by9"><iframe allowfullscreen="true" class="embed-responsive-item" src="https://livestream.com/accounts/25937168/events/7713617/player?enableInfoAndActivity=true&amp;defaultDrawer=&amp;autoPlay=true&amp;mute=false"></iframe></div>

<center>
<h1>Calendar</h1>
</center>
<div class="gcalendar embed-responsive embed-responsive-4by3">

{{attach_library('makerparktheme/gcalendar')}}

  <iframe src="https://calendar.google.com/calendar/embed?src=antounsawires%40gmail.com&ctz=America%2FNew_York" style="border: 20px "width="600" height="650" frameborder="10" scrolling="no"></iframe>


  <style class="stylish" type="text/css">



  .gcalendar {
	-webkit-filter: invert(0%) hue-rotate(180deg);
	filter: invert(60%) hue-rotate(180deg);
	background-color: #333333;

  #onegoogbar a[href^="https://accounts.google.com"],
  #onegoogbar a[href="javascript:_SR_backToCalendar();"],
  #gb a[href^="https://accounts.google.com"],

	-webkit-filter: invert(100%) hue-rotate(180deg);
	filter: invert(100%) hue-rotate(180deg);
  }

</style>
</div>

{% if page.footer %}
  <footer role="contentinfo">
    {{ page.footer }}
  </footer>
{% endif %}

  </div>









