<html lang="en">
<head>

<meta name="viewport" content="width=device-width, initial-scale=1">

<link rel="stylesheet" href="node_modules/bootstrap/dist/css/bootstrap.css">
<script src="js/jquery-3.2.1.min.js"></script>

</head>
<body>

    <header role="banner">
      {{ page.header }}
    </header>

<div class="layout-container">


  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">


      <div class="embed-responsive embed-responsive-16by9">
      <iframe class="embed-responsive-item" id="ls_embed_1515455866" src="https://livestream.com/accounts/25937168/events/7713617/player?enableInfoAndActivity=true&defaultDrawer=&autoPlay=true&mute=false"frameborder="0" scrolling="no" allowfullscreen> </iframe><script type="text/javascript" data-embed_id="ls_embed_1515455866" src="https://livestream.com/assets/plugins/referrer_tracking.js"></script>
      </div>


      {{ page.content }}
    </div>{# /.layout-content #}

    {% if page.sidebar %}
        <aside class="layout-sidebar-first" role="complementary">
          {{ page.sidebar }}

          <h1>Test Text2</h1>

          <style>
          .layout-sidebar-first{
            width: 300px;
            height: 500px;
            padding-right: 15px;



          }</style>

        </aside>
      {% endif %}


  </main>

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# /.layout-container #}
</body>
</html>
